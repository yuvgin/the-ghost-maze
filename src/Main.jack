class Main {
	/**
	A main function that runs the game.
	*/
	function void main() {
		var MazeGame game;
		do GameAssets.initSeed(9); // initialize seed for pseudorandom generator
		let game = MazeGame.new();
		do game.run();
		do game.dispose(); // dispose game memory
		return;
		
	}
}

    